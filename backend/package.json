{
  "name": "hawker-opportunity-score-backend",
  "version": "1.0.0",
  "description": "Backend API for Hawker Opportunity Score Platform",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts",
    "ingest:subzones": "tsx src/services/ingest/subzones.ura.ts",
    "ingest:population": "tsx src/services/ingest/population.census2020.ts",
    "ingest:hawker": "tsx src/services/ingest/hawker.nea.ts",
    "ingest:mrt": "tsx src/services/ingest/mrt.ts",
    "ingest:bus": "tsx src/services/ingest/busStops.ts",
    "ingest:all": "npm run ingest:subzones && npm run ingest:population && npm run ingest:hawker && npm run ingest:mrt && npm run ingest:bus",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "papaparse": "^5.5.3",
    "prisma": "^5.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/papaparse": "^5.3.16",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
